//write a program using addition operator
//header file
#include <stdio.h>
//function heading
void main()
{
  //variable declaration
  int variable_1= 43,variable_2= 27,sum;
  //assignment statement
  sum= variable_1+variable_2;
  //output statement
  printf("%d+%d=%d",variable_1,variable_2,sum);
   }
